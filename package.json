  "type": "module",
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "__tests__/jest.tsconfig.json"
      }
    },
    "setupFiles": [
      "./config/envModules.ts"
    ],
    "testEnvironment": "node",
    "testTimeout": 15000
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
