{
  "name": "@wander-suite/wander-postgres",
  "version": "1.0.3",
  "description": "A generalized postgres instance which enables db querying for Wander",
  "main": "src/index.ts",
  "scripts": {
    "ci-test": "jest --runInBand --verbose",
    "test": "docker-compose up -d && sleep 5 && jest --watch --runInBand --verbose; docker compose down",
    "lint": "eslint --ext .js,.ts .",
    "prepublishOnly": "tsc && rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wander-suite/wander-postgres.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wander-suite/wander-postgres/issues"
  },
  "homepage": "https://github.com/wander-suite/wander-postgres#readme",
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "dependencies": {
    "@jest/globals": "^29.5.0",
    "@types/lodash": "^4.14.194",
    "lodash": "^4.17.21",
    "pg": "^8.10.0",
    "sql-template-strings": "^2.2.2",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.5",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.3",
    "@types/pg": "^8.6.6",
    "babel-jest": "^29.5.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.33.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.3",
    "rollup-plugin-dts": "^5.3.1",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1"
  },
  "type": "module",
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "__tests__/jest.tsconfig.json"
      }
    },
    "setupFiles": [
      "./config/envModules.ts"
    ],
    "testEnvironment": "node",
    "testTimeout": 15000
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/preset-typescript"
    ]
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
